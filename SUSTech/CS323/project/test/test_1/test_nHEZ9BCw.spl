int Next(char T, int next) {
    int i = 1;
    int j = 0;

    next[1] = 0;
    while (i < strlen(T)) {
        if (j == 0 || T[i - 1] == T[j - 1]) {
            i = i + 1;
            j = j + 1;
            next[i] = j;
        } else {
            j = next[j];
        }
    }
    return 0;
}

int KMP(char S, char T) {
    int next;
    int i = 1;
    int j = 1;

    Next(T, next);

    while (i <= strlen(S) && j <= strlen(T)) {
        if (j == 0 || S[i - 1] == T[j - 1]) {
            i = i + 1;
            j = j + 1;
        } else {
            j = next[j];
        }
    }
    if (j > strlen(T)) {
        return i - strlen(T);
    }
    return -1;
}